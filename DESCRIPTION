Encoding: UTF-8
LazyData: true
Depends:
    R (>= 2.10)
Imports:
    geojsonsf,
	methods,
	sf,
	tidyr,
	utils,
    V8
Suggests:
    testthat,
    covr
RoxygenNote: 6.0.1
